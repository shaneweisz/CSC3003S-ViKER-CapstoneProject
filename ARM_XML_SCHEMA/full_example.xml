<arm>

	<entity name="Professor" type="Entity">
		<attribute type="attr" data_type="OID">self</attribute>
		<attribute type="attr" data_type="INT">pnum</attribute>
		<attribute type="attr" data_type="STRING">pname</attribute>
        <attribute type="attr" data_type="STRING">office</attribute>
		<attribute type="attr" data_type="OID">department</attribute>
		<constraint type="pk">self</constraint>
        <constraint type="fk" fk="department" references="Department">dept</constraint>
        <constraint type="path_fd" target="self">
          <fd_attrib>pnum</fd_attrib>
        </constraint>
	</entity>

	<entity name="Department" type="Entity">
		<attribute type="attr" data_type="OID">self</attribute>
		<attribute type="attr" data_type="STRING">dcode</attribute>
		<attribute type="attr" data_type="STRING">dname</attribute>
		<constraint type="pk">self</constraint>
        <constraint type="path_fd" target="self">
          <fd_attrib>dcode</fd_attrib>
        </constraint>
	</entity>

    <entity name="Supplier" type="Entity">
		<attribute type="attr" data_type="OID">self</attribute>
		<attribute type="attr" data_type="STRING">sname</attribute>
		<constraint type="pk">self</constraint>
        <constraint type="path_fd" target="self">
          <fd_attrib>sname</fd_attrib>
        </constraint>
	</entity>

    <entity name="Part" type="Entity">
		<attribute type="attr" data_type="OID">self</attribute>
		<attribute type="attr" data_type="INT">PID</attribute>
		<constraint type="pk">self</constraint>
        <constraint type="path_fd" target="self">
          <fd_attrib>PID</fd_attrib>
        </constraint>
	</entity>

    <entity name="Supplies" type="Entity">
		<attribute type="attr" data_type="OID">self</attribute>
        <attribute type="attr" data_type="OID">sname</attribute>
		<attribute type="attr" data_type="OID">PID</attribute>
		<constraint type="pk">self</constraint>
        <constraint type="path_fd" target="self">
          <fd_attrib>sname</fd_attrib>
          <fd_attrib>PID</fd_attrib>
        </constraint>
        <constraint type="fk" fk="sname" references="Supplier">supplier</constraint>
        <constraint type="fk" fk="PID" references="Part">part</constraint>
	</entity>

    <entity name="Payment" type="Entity">
		<attribute type="attr" data_type="OID">self</attribute>
		<attribute type="attr" data_type="OID">LID</attribute>
        <attribute type="attr" data_type="DATE">paytime</attribute>
        <attribute type="attr" data_type="INT">amt</attribute>
        <attribute type="attr" data_type="STRING">type</attribute>
		<constraint type="pk">self</constraint>
        <constraint type="path_fd" target="self">
          <fd_attrib>LID</fd_attrib>
          <fd_attrib>paytime</fd_attrib>
        </constraint>
        <constraint type="fk" fk="LID" references="Loan">loan</constraint>
	</entity>

    <entity name="Loan" type="Entity">
		<attribute type="attr" data_type="OID">self</attribute>
		<attribute type="attr" data_type="OID">LID</attribute>
        <attribute type="attr" data_type="STRING">Other</attribute>
		<constraint type="pk">self</constraint>
        <constraint type="path_fd" target="self">
          <fd_attrib>LID</fd_attrib>
        </constraint>
	</entity>


</arm>
